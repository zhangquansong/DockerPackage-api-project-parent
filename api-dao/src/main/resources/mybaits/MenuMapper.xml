<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.clt.api.dao.MenuDao">
    <resultMap id="BaseResultMap" type="com.clt.api.entity.Menu">
        <id column="id" property="id" jdbcType="BIGINT"/>
        <result column="menuName" property="zjy_menu_name" jdbcType="VARCHAR"/>
        <result column="parentId" property="zjy_parent_id" jdbcType="BIGINT"/>
    </resultMap>

    <select id="getMenus" resultType="com.clt.api.entity.Menu">
        SELECT
         *
        FROM zjy_sys_menu
    </select>
</mapper>
